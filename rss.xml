---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
{% assign notes = site.notes  %}

 <title>{{ site.title | xml_escape }}</title>
<link href="{{ site.url | xml_escape }}{{ site.baseurl | xml_escape }}/rss.xml" rel="self"/>
<link href="{{ site.url | xml_escape }}{{ site.baseurl | xml_escape }}/" />
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>{{ site.url | xml_escape }}</id>
<author>
  <name>{{ site.author | xml_escape }}</name>
</author>

{% for note in notes %}
<entry>
  <title>{{ note.title | xml_escape }}</title>
  <link href="{{ site.url | xml_escape }}{{ site.baseurl | xml_escape }}{{ note.url | xml_escape }}" />
  <updated>{{ note.date | date_to_xmlschema }}</updated>
  <id>{{ site.url | xml_escape }}{{ note.id | xml_escape }}</id>
  <content type="html">{{ note.content | xml_escape }}</content>
</entry>
{% endfor %}

</feed>