---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
{% assign notes = site.notes  %}

 <title>{{ site.title }}</title>
 <link href="{{ site.url }}{{ site.baseurl }}/rss.xml" rel="self"/>
 <link href="{{ site.url }}{{ site.baseurl }}/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>{{ site.url }}</id>
 <author>
   <name>{{ site.author }}</name>
 </author>

 {% for note in notes %}
 <entry>
   <title>{{ note.title }}</title>
   <link href="{{ site.url }}{{ site.baseurl }}{{ note.url }}"/>
   <updated>{{ note.date | date_to_xmlschema }}</updated>
   <id>{{ site.url }}{{ note.id }}</id>
   <content type="html">{{ note.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>